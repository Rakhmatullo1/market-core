<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">


    <changeSet id="1" author="Raxmatullo">
        <preConditions onFail="MARK_RAN">
            <foreignKeyConstraintExists foreignKeyName="fk_product_history_id"/>
        </preConditions>
        <dropForeignKeyConstraint baseTableName="history" constraintName="fk_product_history_id"/>
    </changeSet>
    <changeSet id="2" author="Raxmatullo">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="history"/>
        </preConditions>
        <dropTable tableName="history"/>
    </changeSet>
</databaseChangeLog>